{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": [">0.3%", "not dead", "not op_mini all"]
          },
          "useBuiltIns": "usage",
          "corejs": 3,
          "modules": false
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
