{
  "version": 2,
  "builds": [
    {
      "src": "packages/nextjs/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "build": {
    "env": {
      "NPM_CONFIG_IGNORE_SCRIPTS": "1",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "nodeVersion": "18.x"
}
